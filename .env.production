# Database
DB_HOST=prod.db.server
DB_PORT=27017
DB_NAME=myapp_prod
DB_USER=myapp_prod_user
DB_PASS=myapp_prod_password

# Server Configuration
PORT=8080
HOST=https://myapp.com

# Logging
LOG_LEVEL=info

# Authentication
JWT_SECRET=production_secret_key
JWT_EXPIRATION=24h

# Third-party Services (e.g., email service, payment gateway)
EMAIL_API_KEY=your_email_service_prod_api_key

# Security
CORS_ORIGIN=https://myapp.com

# Others (based on what your app needs)
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX_REQUESTS=100
